{
  "root": "libs/design/react-native",
  "sourceRoot": "libs/design/react-native/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/design/react-native/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/design/react-native"
      ],
      "options": {
        "jestConfig": "libs/design/react-native/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
