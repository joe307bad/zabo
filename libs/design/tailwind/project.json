{
  "root": "libs/design/tailwind",
  "sourceRoot": "libs/design/tailwind/src",
  "projectType": "library",
  "targets": {
    "watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "tailwindcss -c ./libs/design/tailwind/tailwind.config.js -o ./libs/design/tailwind/src/tailwind.css --watch"
          }
        ]
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "tailwindcss -c ./libs/design/tailwind/tailwind.config.js -o ./libs/design/tailwind/src/tailwind.css --minify"
          }
        ]
      }
    }
  },
  "tags": []
}
